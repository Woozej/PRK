<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ExitController">
   <children>
      <Label stylesheets="@application.css" text="Czy chcesz zakończyć pracę?" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
      <GridPane GridPane.rowIndex="1">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" onAction="#ExitProgram" prefHeight="40.0" prefWidth="80.0" stylesheets="@application.css" text="Tak" GridPane.halignment="CENTER" />
            <Button maxWidth="80.0" mnemonicParsing="false" onAction="#Return" prefHeight="40.0" stylesheets="@application.css" text="Nie" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
         </children>
      </GridPane>
   </children>
   <rowConstraints>
      <RowConstraints prefHeight="100.0" />
      <RowConstraints minHeight="10.0" prefHeight="60.0" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints prefWidth="300.0" />
   </columnConstraints>
</GridPane>
